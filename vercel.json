{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "history.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/assess-risk",
      "dest": "api/index.py"
    },
    {
      "src": "/api/assess-environmental",
      "dest": "api/index.py"
    },
    {
      "src": "/api/history",
      "dest": "api/index.py"
    },
    {
      "src": "/history.html",
      "dest": "/history.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
